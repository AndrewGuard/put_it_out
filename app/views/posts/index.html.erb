 <div class="container">
 	<h2>Showing all posts! </h2>
 	<table>
 	<% @posts.each do |post| %>
 		<tr><td> <a href="/posts/<%= post.id %> "><%= post.title %>-Votes: <%=post.vote_count %></a></td><br>
 		<td> <%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td></tr>
 	<% end %>
 	</table>
 </div>


